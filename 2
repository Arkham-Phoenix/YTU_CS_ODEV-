#include<stdio.h>

int asalmi(int sayi){

int i;

for(i=2;i<sayi;i++){
    if(sayi % i == 0){
        return 0;
    }
    if(sayi == 2){
        return 1;
    }
}
return 1;
}

int main(){

int sayi1,sayi2,asallar;
int toplam = 0;

printf("Lutfen iki adet birbirinden farkli asal sayi giriniz.\n");
printf("1. Sayi:");
scanf("%d",&sayi2);
printf("2. Sayi:");
scanf("%d",&sayi1);

for(asallar=sayi2;asallar<=sayi1;asallar++){
    if(asalmi(asallar) != 0){
        toplam += asallar;
    }
}
printf("Toplam:%d",toplam);
return 0;
}
